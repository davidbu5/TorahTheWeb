{
  "name": "TorahTheWeb",
  "description": "Taking Torah learning thorugh the web to the next level",
  "version": "0.0.1",
  "permissions": [
    "notifications",
    "storage",
    "tabs",
    "background"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "browser_action": {
    "default_title": "TorahTheWeb",
    "default_icon": {
      "19": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png"
  },
  "manifest_version": 2
}
